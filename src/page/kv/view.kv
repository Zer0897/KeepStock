        InventoryView:
            name: '1'
            text: self.name.title()

        InventoryView:
            name: '2'
            text: self.name.title()

        InventoryView:
            name: '3'
            text: self.name.title()

        InventoryView:
            name: 'me'
            text: self.name.title()