#:kivy 1.10.1

#:import screenmanager kivy.uix.screenmanager


<InventoryViewPage>:
    name: 'view'
    sub_pages: [NavigationItem('Me', 'me'), NavigationItem('Shelf', 'shelf')]

    ScreenManager:
        id: manager
        transition: screenmanager.SlideTransition(direction='up')

        InventoryView:
            name: 'shelf'
            title: self.name.title()

        InventoryView:
            name: 'me'
            title: self.name.title()
