#:kivy 1.10.1

#:import NavigationItem src.widget.navigate.NavigationItem


<MenuPage>:
    name: 'menu'

    PrimaryView:
        ScreenManager:
            id: manager
            current: 'view'
            ViewPage:
            EditPage:
            InPage:
            OutPage:

        NavigationBar:
            id: navbar
            manager: manager
            size_hint_y: None
            height: '130sp'
            items:
                [
                NavigationItem('Edit', 'edit'),
                NavigationItem('In', 'in'),
                NavigationItem('Out', 'out'),
                NavigationItem('View', 'view')
                ]